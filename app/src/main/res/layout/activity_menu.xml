    <Button
        android:id="@+id/goToNotificationsSettingsBtn"
        android:layout_width="249dp"
        android:layout_height="60dp"
        android:layout_marginStart="78dp"
        android:layout_marginTop="536dp"
        android:layout_marginEnd="84dp"
        android:layout_marginBottom="135dp"
        android:background="@drawable/rounded_button_mint"
        android:text="NOTIFICATIONS SETTINGS"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/goToTherapyApproachesBtn"
        android:layout_width="249dp"
        android:layout_height="55dp"
        android:layout_marginStart="81dp"
        android:layout_marginTop="442dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="234dp"
        android:background="@drawable/rounded_button_purple"
        android:text="Therapy Approaches"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/goToCheckYourMedicationsBtn"
        android:layout_width="246dp"
        android:layout_height="55dp"
        android:layout_marginStart="81dp"
        android:layout_marginTop="253dp"
        android:layout_marginEnd="84dp"
        android:layout_marginBottom="423dp"
        android:background="@drawable/rounded_button_purple"
        android:text="CHECK YOUR MEDICATIONS"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/goToYourTherapistBtn"
        android:layout_width="249dp"
        android:layout_height="55dp"
        android:layout_marginStart="81dp"
        android:layout_marginTop="347dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="329dp"
        android:background="@drawable/rounded_button_mint"
        android:text="YOUR THERAPIST"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/homeBtn"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="360dp"
        android:layout_marginBottom="677dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_home_24" />

</androidx.constraintlayout.widget.ConstraintLayout>