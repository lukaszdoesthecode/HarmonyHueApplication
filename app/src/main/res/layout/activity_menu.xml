<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuActivity"
    android:background="#F8F8F8">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_medium"
        android:text="Log out"
        tools:layout_editor_absoluteX="343dp"
        tools:layout_editor_absoluteY="30dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/baseline_logout_24"
        tools:layout_editor_absoluteX="310dp"
        tools:layout_editor_absoluteY="22dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_medium"
        android:text="Home"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="30dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="185dp"
        android:layout_height="184dp"
        app:srcCompat="@drawable/logo"
        tools:layout_editor_absoluteX="113dp"
        tools:layout_editor_absoluteY="0dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="249dp"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/rounded_button_purple"
        android:text="NOTIFICATIONS AND SETTINGS"
        app:layout_constraintTop_toBottomOf="@+id/button"
        tools:layout_editor_absoluteX="81dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="249dp"
        android:layout_height="55dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/rounded_button_purple"
        android:text="MOOD STATISTICS"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        tools:layout_editor_absoluteX="81dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="249dp"
        android:layout_height="55dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/rounded_button_mint"
        android:text="PERSONALIZED ADVICES"
        app:layout_constraintTop_toBottomOf="@+id/button6"
        tools:layout_editor_absoluteX="81dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="249dp"
        android:layout_height="55dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/rounded_button_purple"
        android:text="Therapy Quiz"
        app:layout_constraintTop_toBottomOf="@+id/button4"
        tools:layout_editor_absoluteX="81dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/decorator_grass"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="436dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="249dp"
        android:layout_height="55dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/rounded_button_mint"
        android:text="PSYCHOLOGY VADEMECUM"
        app:layout_constraintTop_toBottomOf="@+id/button5"
        tools:layout_editor_absoluteX="81dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="246dp"
        android:layout_height="55dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/rounded_button_purple"
        android:text="MEDICATIONS"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        tools:layout_editor_absoluteX="81dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="249dp"
        android:layout_height="55dp"
        android:background="@drawable/rounded_button_mint"
        android:text="YOUR THERAPIST"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="277dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/baseline_home_24"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="19dp" />

</androidx.constraintlayout.widget.ConstraintLayout>