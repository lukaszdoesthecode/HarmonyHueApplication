<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F8F8F8"
    tools:context=".RegisterActivity">

    <TextView
        android:id="@+id/loginText2"
        android:layout_width="189dp"
        android:layout_height="193dp"
        android:layout_marginStart="192dp"
        android:layout_marginTop="210dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="328dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Create new Account."
        android:textAlignment="center"
        android:textColor="@color/lavender"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="149dp"
        android:layout_height="39dp"
        android:layout_marginStart="213dp"
        android:layout_marginTop="291dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="401dp"
        android:fontFamily="@font/poppins_light"
        android:text="Already Registered? Log in here."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/logoImage1"
        android:layout_width="297dp"
        android:layout_height="216dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="553dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.166"
        app:srcCompat="@drawable/logo" />

    <EditText
        android:id="@+id/passwordEDlogin"
        android:layout_width="353dp"
        android:layout_height="47dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="447dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="237dp"
        android:background="@drawable/rounded_button_gray"
        android:ems="10"
        android:inputType="textPassword"
        android:padding="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nameTVlogin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="345dp"
        android:layout_marginEnd="345dp"
        android:layout_marginBottom="363dp"
        android:fontFamily="@font/poppins_light"
        android:text="EMAIL"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/nameED"
        android:layout_width="353dp"
        android:layout_height="47dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="368dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="316dp"
        android:background="@drawable/rounded_button_gray"
        android:ems="10"
        android:inputType="text"
        android:padding="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/passwordTVlogin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="424dp"
        android:layout_marginEnd="306dp"
        android:layout_marginBottom="284dp"
        android:fontFamily="@font/poppins_light"
        android:text="PASSWORD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnRegisterRA"
        android:layout_width="355dp"
        android:layout_height="52dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="518dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="161dp"
        android:background="@drawable/rounded_button_purple"
        android:fontFamily="@font/poppins_medium"
        android:text="register"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnRegisterGoogleRA"
        android:layout_width="355dp"
        android:layout_height="52dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="73dp"
        android:background="@drawable/rounded_button_terrible"
        android:fontFamily="@font/poppins_medium"
        android:text="register with google"
        app:layout_constraintBottom_toBottomOf="@+id/btnLogInRA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnRegisterRA" />

    <Button
        android:id="@+id/btnLogInRA"
        android:layout_width="355dp"
        android:layout_height="52dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="663dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/rounded_button_mint"
        android:fontFamily="@font/poppins_medium"
        android:text="log in"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="229dp"
        android:layout_height="193dp"
        android:layout_marginTop="128dp"
        android:layout_marginEnd="244dp"
        android:layout_marginBottom="410dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/decorator2" />


</androidx.constraintlayout.widget.ConstraintLayout>